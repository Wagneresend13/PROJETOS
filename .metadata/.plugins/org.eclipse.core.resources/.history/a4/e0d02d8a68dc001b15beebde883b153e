package REVISAODESAFIOHERANCA5_17;

public class ContaReceber extends Conta{
	
	Cliente cliente;
	
	
	ContaReceber(){
		
	}
	
	ContaReceber(Cliente cliente , String descricao , double valor , String dataVencimento){
		
		this.cliente = cliente;
		this.setDescricao(descricao);
		this.setValor(valor);
		this.setDataVencimento(dataVencimento);
	}
	
	public void cancelar() {
		
		if(this.getValor() > 50000) {
			System.out.println(" Valor da Conta maior que 50000 ! , não é possível cancelar");
		}else {
			super.cancelar();
		}
	}
	
	

}
