package Metodo3;

public class ContaCorrente {
	
	
	float saldo;
	
	void  definirSaldoIniciado(float valor) {
		
		saldo = valor;
				
	}
	
	
	float depositar (float valor) {
		
		saldo = valor + saldo;
		return saldo;
	}
	
	float sacar( float valor) {
		
		
		if(valor > saldo) {
			System.out.println(" Valor maior que a quantia do saldo");
		}else{

		saldo = saldo - valor;
		return saldo;
		}

	}
	

}
