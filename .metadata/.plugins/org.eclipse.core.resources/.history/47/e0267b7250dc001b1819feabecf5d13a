package Seguravel;

public class PrincipalDesafio {

	public static void main(String[] args) {
		
		CorretoraSeguros corretora = new CorretoraSeguros();
		
		Carro meucarro = new Carro(45000d , 2020);
		Imovel minhaCasa = new Imovel(320,920000);
		minhaCasa.setAreaContruida(320);
		minhaCasa.setValorMercado(92000);
	
		
		corretora.fazerPropostaSegura(minhaCasa);
		corretora.fazerPropostaSegura(meucarro);
		
		
		
		

	}

}
